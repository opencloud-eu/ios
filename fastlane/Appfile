app_identifier(ENV['OPENCLOUD_APP_IDENTIFIER']) # The bundle identifier of your app
apple_id(ENV['OPENCLOUD_APPLE_ID']) # Your Apple email address

itc_team_id(ENV['OPENCLOUD_ITC_TEAM']) # iTunes Connect Team ID
team_id(ENV['OPENCLOUD_TEAM_ID']) # Developer Portal Team ID

